{
  "name": "metastratum",
  "version": "0.1.0",
  "description": "Interactive web-based terrain analysis platform with 3D visualization and geospatial analysis algorithms",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\""
  },
  "keywords": [
    "terrain-analysis",
    "GIS",
    "DEM",
    "digital-elevation-model",
    "3D-visualization",
    "webgpu",
    "three.js",
    "geospatial",
    "computational-geometry",
    "nextjs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "^9.95.0",
    "@react-three/fiber": "^8.15.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-leaflet": "^4.2.1",
    "three": "^0.160.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jsdom": "^23.2.0",
    "prettier": "^3.2.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^1.2.0"
  }
}
